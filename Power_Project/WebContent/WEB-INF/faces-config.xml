<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>stud</managed-bean-name>
  <managed-bean-class>Poo.CashPower</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>Editstud</managed-bean-name>
  <managed-bean-class>Poo.CashPower</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/Page/Save.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>Liste</from-outcome>
   <to-view-id>/Page/Liste.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>start</from-outcome>
   <to-view-id>/start.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/Page/Liste.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>Save</from-outcome>
   <to-view-id>/Page/Save.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>start</from-outcome>
   <to-view-id>/start.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/start.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>Save</from-outcome>
   <to-view-id>/Page/Save.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>startS</from-outcome>
   <to-view-id>/PageS/startS.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/PageS/startS.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>start</from-outcome>
   <to-view-id>/start.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>SaveS</from-outcome>
   <to-view-id>/PageS/SaveS.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/PageS/SaveS.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>startS</from-outcome>
   <to-view-id>/PageS/startS.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>ListeS</from-outcome>
   <to-view-id>/PageS/ListeS.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>startS1</from-outcome>
   <to-view-id>/PageS/startS.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/PageS/ListeS.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>SaveS</from-outcome>
   <to-view-id>/PageS/SaveS.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>startS</from-outcome>
   <to-view-id>/PageS/startS.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
 </application>
</faces-config>
